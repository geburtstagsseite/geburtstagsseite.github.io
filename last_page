        function myFunc() {
            if (document.getElementById('Richtig').checked === true && document.getElementById('Richtig2').checked == true && document.getElementById('Richtig3').checked == true ) {
                document.getElementById('richtig4').style = "visibility:visible";
            }
            else if (document.getElementById('Richtig3').checked === true) {
                document.getElementById('richtig3').style = "visibility:visible";
            }
            else {
                alert("Nope! Versuchs noch mal!")
            }
        }
